{
  "extends": ["next", "next/core-web-vitals"],
  "rules": {
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }],
    "no-magic-numbers": [
      "warn",
      {
        "ignore": [0, -1, 1, 2, 100],
        "ignoreArrayIndexes": true
      }
    ]
    // "no-console": "warn"
  }
}
